{
  "name": "privacy-center",
  "version": "1.0.0",
  "description": "Privacy notice management system",
  "scripts": {
    "build": "eleventy",
    "start": "eleventy --serve",
    "dev": "eleventy --serve --watch",
    "version-manager": "node version-manager.js",
    "process-versions": "node version-manager.js --process-all",
    "check-status": "node version-manager.js --check-status",
    "ci-prepare": "npm run process-versions && npm run check-status"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "luxon": "^3.4.4",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0"
  },
  "dependencies": {
    "fs-extra": "^11.3.1",
    "gray-matter": "^4.0.3"
  }
}
